msgctxt "model:ir.message,text:account_bank_statement_already_has_lines"
msgid "Statement \"%(statement)s\" already has lines."
msgstr "L'extracte \"%(statement)s\" ja conté línies."

msgctxt "model:ir.message,text:cannot_cancel_statement_line"
msgid "Line \"%(line)s\" cannot be cancelled."
msgstr "La línia \"%(line)s\" no es pot cancel·lar."

msgctxt "model:ir.message,text:cannot_delete_statement"
msgid "Statement \"%(statement)s\" cannot be deleted because it contains lines."
msgstr "L'extracte \"%(statement)s\" no es pot eliminar perquè conté línies."

msgctxt "model:ir.message,text:cannot_delete_statement_line"
msgid ""
"Line \"%(line)s\" cannot be deleted because it is not in Draft or Cancelled "
"state."
msgstr ""
"No es pot eliminar la línia \"%(line)s\" perquè no es troba en estat "
"esborrany o estat cancel·lat."

msgctxt "model:ir.message,text:currency_modify"
msgid ""
"You cannot modify the currency to journal that already has a statement on "
"date %(date)s"
msgstr ""
"No pot modificar la moneda al diari que ja té un extracte en la data "
"%(date)s"

msgctxt "model:ir.message,text:different_amounts"
msgid ""
"Moves Amount \"%(moves_amount)s\" does not match Amount \"%(amount)s\" in "
"line \"%(line)s\"."
msgstr ""
"L'import dels movimients \"%(moves_amount)s\" no coincideix amb l'import "
"\"%(amount)s\" a la línia \"%(line)s\"."

msgctxt "model:ir.message,text:format_error"
msgid ""
"The supplied file does not have the expected format.\n"
"This is the technical error returned by parser:\n"
"%(error)s."
msgstr ""
"L'arxiu subministrat no té el format esperat.\n"
"Aquest és l'error tècnic retornat per l'analitzador:\n"
"%(error)s."

msgctxt "model:ir.message,text:invalid_account_payment_type"
msgid ""
"Can not set Payment Type in move line \"%(payment)s\" because account is not"
" Payable nor Receivable."
msgstr ""
"No es por establir el tipus de pagament a l'apunt contable \"%(payment)s\" "
"perquè el compte no és a pagar o a cobrar. "

msgctxt "model:ir.message,text:invalid_date"
msgid "Invalid string for date: %(date)s"
msgstr "Cadena no vàlida per a la data: %(date)s"

msgctxt "model:ir.message,text:invalid_number"
msgid "Invalid string for number: %(number)s"
msgstr "Cadena no vàlida per al número: %(number)s"

msgctxt "model:ir.message,text:line_reconciled"
msgid ""
"Line \"%(line)s\" already reconciled with bank statement line "
"\"%(statment_line)s\" with amount \"%(amount)s\". Please remove bank "
"conciliation and try again."
msgstr ""
"La línia \"%(line)s\" ja està conciliada amb la línia d'extracte bancari "
"\"%(statment_line)s\" amb la quantitat \"%(amount)s\". Si us plau, elimini "
"la conciliació bancària i torni a intentar-ho."

msgctxt "model:ir.message,text:missing_columns"
msgid "Missing columns in line: %(columns)s"
msgstr "Falten les columnes a la línia: %(columns)s"

msgctxt "model:ir.message,text:modifiy_with_related_model"
msgid ""
"It is not possible to modify the field \"%(field)s\" as the payment type "
"\"%(type)s\" is used on %(model)s \"%(name)s\""
msgstr ""
"No és possible modificar el camp \"%(field)s\" perquè el tipus de pagament "
"\"%(type)s\" s’utilitza a %(model)s \"%(name)s\""

msgctxt "model:ir.message,text:statement_already_has_lines"
msgid ""
"You cannot import a bank statement file in a bank  statement with lines."
msgstr ""
"No podeu importar un fitxer d'extracte bancari en un extracte bancari amb "
"línies."

msgctxt "model:ir.message,text:unicode_error"
msgid ""
"The supplied file has Unicode Error and can not load. Check that the file is"
" utf-8."
msgstr ""
"El fitxer té un error Unicode i no es pot carregar. Comproveu que el fitxer "
"sigui utf-8."

